{
  "name": "foxed",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "node dist/server/entry-server",
    "build": "tsc && vite build --outDir dist-SPA/",
    "build:client": "tsc && vite build --outDir dist/client",
    "build:server": "tsc && vite build --ssr src/entry-server.tsx --outDir dist/server",
    "build:SSR": "npm run build:client && npm run build:server",
    "generate": "vite build --outDir dist/static && npm run build:server && node prerender",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "serve": "vite preview dist/static",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "vite-plugin-pwa": "^0.19.7",
    "vitest": "^1.5.0"
  },
  "dependencies": {
    "@hono/node-server": "^1.7.0",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.7.2",
    "express": "^4.18.2",
    "jwt-decode": "^4.0.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.4",
    "react-icons": "^4.11.0",
    "react-router-dom": "^6.17.0",
    "react-window": "^1.8.10",
    "ts-node": "^10.9.2"
  }
}
